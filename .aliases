alias al_dot="alias dotfiles='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=/home/$USER'"
alias al_dotp="alias dotp='dotfiles commit -am update && dotfiles push'"
alias al_rs="alias rs='rsync --info=progress2 -au'"
alias al_can="alias cansetup='sudo ip link set can0 type can bitrate 125000 && sudo ip link set up can0'"
alias al_vpn="alias vpn='sudo openvpn --config /home/$USER/.config/vpn/vpn.ovpn'"
alias al_wg="alias wg='sudo wg-quick up /home/$USER/.config/wireguard/wg0.conf'"
alias al_lll="alias lll='tree -fiql --dirsfirst --noreport'"
alias al_py="alias py='python3'"
alias al_python="alias python='python3'"